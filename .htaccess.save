SetEnv HTTP_DATABASE_USERNAME "root"
SetEnv HTTP_DATABASE_PASSWORD "MyPassword"
SetEnv HTTP_DATABASE_NAME "COP4331"

RewriteEngine On

RewriteRule ^users/([1-9]+[0-9]*)/contacts/$  API/contacts/SearchContact.php?userID=$1 [L]

