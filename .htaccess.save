RewriteRule ^users/([1-9]+[0-9]*)/contacts/([A-Za-z]+)/$  API/contacts/SearchContact.php?userID=$1&lastname=$2 [L]

RewriteRule ^users/([1-9]+[0-9]*)/contacts/([A-Za-z]+)/$  API/contacts/SearchContact.php?userID=$1&email=$2 [L]

RewriteRule ^users/([1-9]+[0-9]*)/contacts/([A-Za-z]+)/$  API/contacts/SearchContact.php?userID=$1&phone=$2 [L]

RewriteRule ^users/([1-9]+[0-9]*)/contacts/([A-Za-z]+)/([A-Za-z]+)/$  API/contacts/SearchContact.php?userID=$1&firstname=$2&lastname=$3 [L]

